{
  "name": "poodle-surf",
  "main": "lib/index.js",
  "version": "10.0.0-alpha.0",
  "private": true,
  "scripts": {
    "run-ios": "diez compile -t ios -o ios -d --cocoapods",
    "build-ios": "diez compile -t ios -o ios --cocoapods && cd ios && pod install",
    "build-ios-ci": "./scripts/build-ios-ci.sh",
    "run-android": "diez compile -t android -o android -d",
    "build-android": "diez compile -t android -o android",
    "run-web": "diez compile -t web -o web -d",
    "build-web": "diez compile -t web -o web --baseUrl /ds/ --staticRoot web/public/ds && cd web && yarn build"
  },
  "dependencies": {
    "@diez/designsystem": "^10.0.0-alpha.0",
    "@diez/engine": "^10.0.0-alpha.0"
  },
  "devDependencies": {
    "diez": "^10.0.0-alpha.0"
  }
}
