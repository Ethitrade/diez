{{#each styleFonts as |fontDeclaration|}}
@font-face {
  {{#each fontDeclaration}}
  {{this.[0]}}: {{{this.[1]}}};
  {{/each}}
}
{{/each}}

:root {
  {{#each styleVariables}}
  --{{this.name}}: {{{this.value}}};
  {{#if this.isNumber}}
  --{{this.name}}-px: {{{this.value}}}px;
  --{{this.name}}-pt: {{{this.value}}}pt;
  --{{this.name}}-em: {{{this.value}}}em;
  --{{this.name}}-rem: {{{this.value}}}rem;
  {{/if}}
  {{/each}}
}

{{#each styleRuleGroups as |group|}}
.{{group.name}} {
  {{#each group.values}}
  {{this.[0]}}: {{{this.[1]}}};
  {{/each}}
}
{{/each}}
