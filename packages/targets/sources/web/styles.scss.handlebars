{{#each styleFonts as |fontDeclaration|}}
@font-face {
  {{#each fontDeclaration}}
  {{this.[0]}}: {{{this.[1]}}};
  {{/each}}
}
{{/each}}

{{#each styleVariables}}
${{this.name}}: {{{this.value}}};
{{#if this.isNumber}}
${{this.name}}-px: #{$ {{~this.name~}} }px;
${{this.name}}-pt: #{$ {{~this.name~}} }pt;
${{this.name}}-em: #{$ {{~this.name~}} }em;
${{this.name}}-rem: #{$ {{~this.name~}} }rem;
{{/if}}
{{/each}}

{{#each styleRuleGroups as |group|}}
@mixin {{group.name}} {
  {{#each group.values}}
  {{this.[0]}}: {{{this.[1]}}};
  {{/each}}
}
{{/each}}
